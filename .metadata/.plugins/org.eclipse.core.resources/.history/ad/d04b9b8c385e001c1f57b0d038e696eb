package constructor;

public class BasketballPlayer {
    public int height;
	public int jumpHeight;// 변수는 첫단어 첫글자는 소문자, 이후 단어 첫글자는 대문자 나머지는 소문자로 작성
	// 위 표기법을 카멜케이스라고 부릅니다.
	
	//생성자 생성
	public BasketballPlayer(int h, int j) {
		height = h;
		jumpHeight = j;
		System.out.println("생성된 선수의 키 : " + height + ", 점프력 : " + jumpHeight);
	}
}
